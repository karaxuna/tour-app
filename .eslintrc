{
    "rules": {
        "no-mixed-spaces-and-tabs": 1,
        "indent": 0,
        "array-bracket-spacing": 0,
        "semi": 0,
        "eol-last": 0,
        "no-var": 0,
        "no-unused-vars": 0,
        "no-console": 0,
        "space-before-function-paren": ["always", "never"]
    },
    "parserOptions": {
        "ecmaVersion": 8,
        "sourceType": "module",
        "ecmaFeatures": {
            "modules": true,
            "arrowFunctions": true,
            "restParams": true,
            "spread": true,
            "classes": true,
            "objectLiteralComputedProperties": true,
            "experimentalObjectRestSpread": true,
            "destructuring": true,
            "templateStrings": true
        }
    }
}